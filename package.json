{
  "name": "webpack-react-redux-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for a webpack, react, redux project with express and es2015",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV='development' node server",
    "start:dist": "npm run build && cross-env PORT=3000 NODE_ENV='production' node server",
    "build": "npm run clean && cross-env NODE_ENV='production' webpack --config ./webpack.production.config.js --progress --profile --colors -p",
    "clean": "rimraf dist",
    "test": "cross-env NODE_ENV='test' nyc -e .jsx tape --require babel-register 'tests/**/*.*'",
    "viewcov": "cross-env NODE_ENV='test' nyc tap tests/**/*.spec.jsx --coverage --coverage-report=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gruberchris/webpack-react-redux-boilerplate.git"
  },
  "keywords": [
    "webpack",
    "react",
    "redux",
    "express",
    "boilerplate",
    "javascript"
  ],
  "author": "Chris Gruber",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gruberchris/webpack-react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/gruberchris/webpack-react-redux-boilerplate#readme",
  "nyc": {
    "include": [
      "app/**"
    ],
    "require": [
      "babel-register"
    ],
    "extension": [
      ".jsx"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jscs": "^3.0.0-beta1",
    "babel-loader": "^6.3.2",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.2",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^2.28.0",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-tap": "^3.1.1",
    "karma-webpack": "^2.0.2",
    "nyc": "^10.1.2",
    "phantomjs-polyfill": "0.0.2",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.2",
    "tap": "^10.2.1",
    "tape": "^4.6.3",
    "tape-jsx-equals": "^1.0.0",
    "tape-redux": "0.0.3",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-saga": "^0.14.3",
    "toastr": "^2.1.2"
  }
}
